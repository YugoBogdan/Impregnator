            <string>///STATES

//OPIS
//ponistiti playerovo kretanje
//ostaviti da moze da se okrece - koprca
//obj_sperm pocinje da klizi prema sredini velikog
//kada dodje do sredine, veliki upada u stanje 3, a sperm ostaje u koprc stanju
//kada igrac napravi zbir razlika uglova svoje usmerenosti veci od 1000, veliki je unisten
//ako igrac to ne uradi za 6 sekundi, veliki ga je pojeo
//ova mehanika bi trebala da se koristi najcesce da se igrac sacuva, izbegne pozicije sa previce
//neprijatelja oko, tj. da se usteka privremeno
//ili da prodje kroz neke prepreke koje su nezaobilazne

///tap counter

if (state == 1){
    if (tap_counter &gt;= 4){
        state = 2;
        sprite_index = spr_enemy_big_damaged;
    }else if (collided){
        scr_game_over();
    }
    
}


if(state == 2){
    if(place_meeting(x,y,obj_sperm)){
        if (obj_sperm.spd &gt;= 10){
            state = 3;
            obj_sperm.state = 'immobile';
            
            //koprcanje
            koprcanje = 0;
            obj_sperm.theta_previous = obj_sperm.theta;
        }
    }
}

if (state == 3){
    obj_sperm.x = x;
    obj_sperm.y = y;

    koprcanje += abs(obj_sperm.theta - obj_sperm.theta_previous);
    obj_sperm.theta_previous = obj_sperm.theta;
    
    if (koprcanje &gt;= 1000){
        obj_sperm.state = 'normal-new';
        state = 1; //mozda bude smaralo kada zabranim da se igrac ne moze kretati tu gde se nalazi veliki
        tap_counter = 0; 
        sprite_index = spr_enemy_big;
    }
}

