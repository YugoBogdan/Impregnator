global.ingame_time += 1/room_speed;

//instance number check

if (instance_number(obj_enemy_parent) &gt;= enemy_limit){
    timeline_index = tl_level1;
    timeline_running = false;
} else if (instance_number(obj_enemy_parent) &amp;&amp; timeline_running = false){
    timeline_running = true;
}

//challegne controller
if (!challenge_up_and_running){
    switch (current_challenge_type){
        case 'checkpoint':
        if (!instance_exists(checkpoint_controller)){
            instance_create(0,0,checkpoint_controller);
            challenge_up_and_running = true;
            global.challenges_count += 1;
        }
        break;

        case 'survival':
        if (!instance_exists(survival_controller)){
            instance_create(0,0,survival_controller);
            challenge_up_and_running = true;
            global.challenges_count += 1;
        }
        break;
        
        case 'boss':
        if (!instance_exists(boss_ctrl)){
            instance_create(room_width/2,0,boss_ctrl);
            challenge_up_and_running = true;
            global.challenges_count += 1;        
        }
        break;
        
        case 'GG':
        show_message("IMPREGNATION COMPLETE" + "#Congratulations! You have beaten the DEMO" + "#We will loop these challenges for now... :)" + "#I hope you've enjoyed it :)");
        current_challenge_number = -1;
        scr_challenge_next(20);
        break;
        
        default:
        show_message("error");
        break;
    }
}
