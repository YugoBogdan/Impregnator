            water = scr_enemy_create('self',obj_sun);
            with (water){
                depth = other.depth-1;
                if (instance_exists(obj_sperm)){
                    if (other.shoot_counter == other.shoot_period){
                        orientation = point_direction(x,y,obj_sperm.x,obj_sperm.y);
                    }
                }
                motion_set(orientation,spd*2+random(7));
                slowing = true;
                //motion_add(other.direction,other.speed);
            }
        }
    }else{
        state = 'normal';
        alarm[1] = shoot_loop_timer;
    }
}
